    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.tyler.api.servlet.LogoutServlet
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>com.tyler.api.servlet.UserServlet
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/UserServlet/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StaticServlet</servlet-name>
        <servlet-class>com.tyler.api.servlet.StaticResourceServlet
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StaticServlet</servlet-name>
        <url-pattern></url-pattern>
        <url-pattern>/RoomServlet/all</url-pattern>
        <url-pattern>/RoomServlet/create</url-pattern>
        <url-pattern>/RoomServlet/deleteRoom</url-pattern>
        <url-pattern>/RoomServlet/addUser</url-pattern>
        <url-pattern>/RoomServlet/removeUser</url-pattern>
        <url-pattern>/Login</url-pattern>
        
        <url-pattern>/styles/stylesheet</url-pattern>
        
        <url-pattern>/js/login</url-pattern>
        <url-pattern>/js/viewRoom</url-pattern>
        <url-pattern>/js/createRoom</url-pattern>
        <url-pattern>/js/deleteRoom</url-pattern>
        <url-pattern>/js/addRoomUser</url-pattern>
        <url-pattern>/js/deleteRoomUser</url-pattern>
        <url-pattern>/js/componentInjector</url-pattern>
        <url-pattern>/js/router</url-pattern>
        <url-pattern>/img/2.jpg</url-pattern>
        <url-pattern>/img/4.webp</url-pattern>
        <url-pattern>/img/7.jpg</url-pattern>
        <url-pattern>/img/background.webp</url-pattern>
        <url-pattern>/img/77.jpg</url-pattern>
        <url-pattern>/img/login2.jpg</url-pattern>
        <url-pattern>/img/logo.png</url-pattern>
        
        
    </servlet-mapping>
</web-app>